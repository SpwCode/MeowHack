{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Meow Hack",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "Spw"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "meowhack.AddonTemplate"
    ]
  },
  "mixins": [
    "meowhack.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
